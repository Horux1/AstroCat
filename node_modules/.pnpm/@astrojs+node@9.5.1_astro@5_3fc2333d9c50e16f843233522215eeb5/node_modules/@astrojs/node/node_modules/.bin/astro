#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/pruebavis/cat/node_modules/.pnpm/astro@5.16.0_@types+node@24_f9825b87d01e2d5966a8e4cf982e9e45/node_modules/astro/node_modules:/mnt/e/pruebavis/cat/node_modules/.pnpm/astro@5.16.0_@types+node@24_f9825b87d01e2d5966a8e4cf982e9e45/node_modules:/mnt/e/pruebavis/cat/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/pruebavis/cat/node_modules/.pnpm/astro@5.16.0_@types+node@24_f9825b87d01e2d5966a8e4cf982e9e45/node_modules/astro/node_modules:/mnt/e/pruebavis/cat/node_modules/.pnpm/astro@5.16.0_@types+node@24_f9825b87d01e2d5966a8e4cf982e9e45/node_modules:/mnt/e/pruebavis/cat/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@5.16.0_@types+node@24_f9825b87d01e2d5966a8e4cf982e9e45/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@5.16.0_@types+node@24_f9825b87d01e2d5966a8e4cf982e9e45/node_modules/astro/astro.js" "$@"
fi
